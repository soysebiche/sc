{
  "version": 2,
  "routes": [
    {
      "src": "/api/data",
      "dest": "/api/data"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(ico|png|jpg|jpeg|gif|svg|css|js))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "comment": "Updated for automatic deployment trigger"
}
